#!/bin/bash
# use commmand

echo "what is type"
whatis type
echo "what is which"
whatis which
echo "what is man"
whatis man
echo "what is apropos"
whatis apropos
echo "what is info"
whatis info
echo "what is whatis"
whatis whatis
echo "what is alias"
whatis alias


echo "What is command"
echo "1.可执行文件2.內建shell命令3.是一个shell函数4.是一个命令别名"

echo "take ls for example"
echo "type ls"
type ls
echo "which ls"
which ls
echo "man ls"
man ls
echo "apropos ls"
apropos ls 
echo "info ls"
info ls 

