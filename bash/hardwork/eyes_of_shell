#!/bin/bash
# eyes of shell

echo "echo *"
echo *

echo "echo /usr/*/shares"
echo /usr/*/share

echo "echo ~"
echo ~

echo "echo $((2*2))"
echo $((2*2))
echo $((1+$((2*3))-$((4/5))))

echo "{}"
echo {1..3}{1..3}

echo "what is printenv"
whatis printenv 
printenv

echo "echo $USER"
echo $USER
echo "echo $ls"
echo $(ls)


